    "keywords": "epson, printer, control, panel",
    "repository": {
        "type": "git",
        "url": "https://github.com/XavierBrassoud/Arduino_Epson_PNL_CE02.git"
    },
    "authors": [
        {
            "name": "Xavier BRASSOUD",
            "email": "contact@xavierbrassoud.fr",
            "url": "https://www.xavierbrassoud.fr/contact",
            "maintainer": true
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/XavierBrassoud/Arduino_Epson_PNL_CE02.git",
    "frameworks": "*",
    "platforms": "*",
    "examples": [
        {
            "name": "blink",
            "base": "examples/blink",
            "files": [
                "platformio.ini",
                "src/Epson_PNL_CE02.h",
                "src/Epson_PNL_CE02.cpp",
                "blink.ino"
            ]
        },
        {
            "name": "buttons",
            "base": "examples/buttons",
            "files": [
                "platformio.ini",
                "src/Epson_PNL_CE02.h",
