                "SPI": "*"
            }
        },
        {
            "name": "buttons",
            "base": "examples/full",
            "files": [
                "platformio.ini",
                "src/Epson_PNL_CE02.h",
                "src/Epson_PNL_CE02.cpp",
                "full.ino"
            ],
            "dependencies": {
                "mathertel/OneButton": "^2.5.0",
                "prenticedavid/MCUFRIEND_kbv": "^3",
                "adafruit/Adafruit GFX Library": "^1",
